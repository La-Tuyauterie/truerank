{
  "compilerOptions": {
    "types": ["node", "@fastify/env"],
    "esModuleInterop": true,
    "moduleResolution": "node",
    "target": "ES2020",
    "module": "CommonJS",
    "strict": true,
    "outDir": "dist",
    "rootDir": "./src",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}